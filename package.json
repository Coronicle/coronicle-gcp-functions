{
  "name": "coronicle",
  "version": "0.0.1",
  "description": "Functions for coronicle hackathon project",
  "repository": "golergka/gcf-typescript-template",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "npx tsc-watch --onSuccess 'npx @google-cloud/functions-framework --target=helloWorldTs'",
    "deploy": "npx tsc && gcloud functions deploy helloWorldTs --runtime nodejs10 --trigger-http"
  },
  "keywords": [
    "Typescript",
    "Template",
    "Google Cloud Functions",
    "GCF",
    "functions",
    "serverless"
  ],
  "author": "sebimarkgraf",
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.1.2",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
